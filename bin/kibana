#! /usr/bin/env bash

source elastic-product.sh

function get() {
    _with_arguments 1 $@
    version=${1}
    mkdir -p ${ELASTIC_PRODUCTS_HOME}
    _maybe_download_product kibana kibana ${version} "-$(_distro)"
    cd ${ELASTIC_PRODUCTS_HOME}/kibana-${version}
}

function clean() {
    _with_arguments 1 $@
    version=${1}
    _clean_product kibana ${version} "-$(_distro)"
}

function versions() {
    _list_versions kibana
}

eval $@

if [[ -z $1 ]]; then
    $0 -h
fi