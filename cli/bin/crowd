#! /usr/bin/env bash

_product_name() {
    echo "crowd"
}

_product_definition() {
    _with_arguments 1 $@
    version=${1}
    echo "-Dproduct=crowd
          -Dproduct.version=${version}
          -Dhttp.port=${HTTP_PORT:-4990}
          -Dajp.port=${AJP_PORT:-4009}
          -Dserver=127.0.0.1"
}

_product_group_id() {
    echo "com.atlassian.crowd"
}

_product_webapp_name() {
    echo "crowd-web-app"
}

source atlassian-product.sh

eval $@

if [[ -z $1 ]]; then
    $0 -h
fi
