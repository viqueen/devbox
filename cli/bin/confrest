#!/usr/bin/env node

"use strict";

const RestClient = require("../lib/rest-client");

const restClient = new RestClient({
  name: "Confluence REST Client",
  apiUrl: "/rest",
  json: true,
  auth: {
    user: "admin",
    pass: "admin",
  },
  context: "/confluence",
  port: 8080,
  host: "localhost"
});

const program = restClient.program();
program.version("1.0.0");
program.parse(process.argv);
